			<expiration max-idle="3600000" interval="15000" />
		</local-cache>
		
		<local-cache name="ProjectBrokerManager@pb" statistics="true">
			<locking isolation="READ_COMMITTED" concurrency-level="1000" acquire-timeout="15000" striping="false" />

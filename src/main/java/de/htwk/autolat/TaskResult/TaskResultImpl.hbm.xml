<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="true">
  <class name="de.htwk.autolat.TaskResult.TaskResultImpl" table="ao_task_result">  
  
    <id name="key" column="id" type="long" unsaved-value="null">
        <generator class="enhanced-sequence">
            <param name="sequence_name">hibernate_unique_key</param>
            <param name="force_table_use">true</param>
            <param name="optimizer">legacy-hilo</param>
            <param name="value_column">next_hi</param>
            <param name="increment_size">32767</param>
            <param name="initial_value">32767</param>
        </generator>
    </id>

	<property  name="maxScoreDate" column="maxscoredate" type="timestamp" />
	<property  name="maxScore" column="maxscore" type="double" />   
	<property  name="maxScoreTaskText" column="maxscoretasktext" type="text" /> 
	<property  name="hasPassed" column="haspassed" type="boolean" /> 
    <property  name="maxScoreSolutionText" column="solutiontext" type="text" />
    
  </class>
</hibernate-mapping>

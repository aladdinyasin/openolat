<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context" 
	xsi:schemaLocation="
  http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
  http://www.springframework.org/schema/context 
  http://www.springframework.org/schema/context/spring-context-3.0.xsd">
  
<!-- show esf in separate tab -->
<bean id="olatsites_esf" class="de.unileipzig.xman.esf.site.ElectronicStudentFileSiteDefinition" scope="prototype" />

<!-- Init the Manager via Spring so that it can register to user delete events -->
<bean class="de.unileipzig.xman.esf.ElectronicStudentFileManager" />

</beans>
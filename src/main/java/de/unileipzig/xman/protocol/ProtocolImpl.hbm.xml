<?xml version="1.0"?>
<!--
/**
 * 
 * Mapping of ProtocolImpl class to database tables
 */
-->
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
  <class name="de.unileipzig.xman.protocol.ProtocolImpl" table="o_xman_protocol">  

    <id name="key" column="protocol_id" type="long" unsaved-value="null">
        <generator class="enhanced-sequence">
            <param name="sequence_name">hibernate_unique_key</param>
            <param name="force_table_use">true</param>
            <param name="optimizer">legacy-hilo</param>
            <param name="value_column">next_hi</param>
            <param name="increment_size">32767</param>
            <param name="initial_value">32767</param>
        </generator>
    </id>
    
    <timestamp name="lastModified" column="lastmodified" />   
  	<property  name="creationDate" column="creationdate" type="timestamp" /> 
    
    <many-to-one name="exam" class="de.unileipzig.xman.exam.ExamImpl" outer-join="auto" cascade="none">  
		<column name="exam_id" not-null="false"/>    	
	</many-to-one>
    
    <many-to-one name="appointment" class="de.unileipzig.xman.appointment.AppointmentImpl" outer-join="auto" cascade="none">  
		<column name="appointment_id" not-null="false"/>    	
	</many-to-one>
    
    <many-to-one name="identity" class="org.olat.basesecurity.IdentityImpl" outer-join="auto" cascade="none">  
		<column name="identity_id" not-null="false"/>    	
	</many-to-one>
	
	<property name="earmarked" column="earmarked" unique="false" type="boolean" not-null="false"/>
	<property name="comments" column="comments" unique="false" type="string" not-null="false" length="16777210"/>
	<property name="grade" column="grade" unique="false" type="string" not-null="false" length="16777210"/>
	<property name="studyPath" column="study_path" type="string" not-null="false" />

  </class>  
</hibernate-mapping>
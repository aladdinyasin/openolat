<?xml version="1.0"?>
<!--
/**
 * 
 * Mapping of ExamImpl class to database tables
 */
-->
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
  <class name="de.unileipzig.xman.exam.ExamImpl" table="o_xman_exam">  

    <id name="key" column="exam_id" type="long" unsaved-value="null">
        <generator class="enhanced-sequence">
            <param name="sequence_name">hibernate_unique_key</param>
            <param name="force_table_use">true</param>
            <param name="optimizer">legacy-hilo</param>
            <param name="value_column">next_hi</param>
            <param name="increment_size">32767</param>
            <param name="initial_value">32767</param>
        </generator>
    </id>
    
    <timestamp name="lastModified" column="lastmodified" />   
  	<property  name="creationDate" column="creationdate" type="timestamp" />   
    
    <property name="name" column="name" unique="false" type="string" not-null="false" />
    
	<many-to-one name="identity" class="org.olat.basesecurity.IdentityImpl" 
		column="identity_id" outer-join="true" cascade="none" not-null="false"/> 
	
    <property name="regStartDate" column="regStartDate" unique="false" type="timestamp" not-null="false"/>
    <property name="regEndDate" column="regEndDate" unique="false" type="timestamp" not-null="false"/>
    <property name="signOffDate" column="signOffDate" unique="false" type="timestamp" not-null="false"/>
    <property name="earmarkedEnabled" column="earmarkedEnabled" type="boolean" unique="false" not-null="false"/>
    <property name="isMultiSubscription" column="isMultiSubscription" type="boolean" unique="false" not-null="false"/>
    <property name="isOral" column="isOral" type="boolean" unique="false" not-null="false"/>
    <property name="comments" column="comments" unique="false" type="string" not-null="false" length="16777210"/>
    	
  </class>  
</hibernate-mapping>